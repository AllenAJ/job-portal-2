create table tbl_jseeker(
    Js_id int(10) NOT NULL AUTO_INCREMENT,
	Username varchar(25) NOT NULL,
    Js_fname varchar(25) NOT NULL,
    Js_lname varchar(25) NOT NULL,
    Js_phno varchar(25) NOT NULL,
    PRIMARY KEY (Js_id),
	FOREIGN KEY (Username) REFERENCES tbl_Login(Username)
)

create table tbl_recruiter(
    R_id int(10) NOT NULL AUTO_INCREMENT,
	Username varchar(25) NOT NULL,
    R_fname varchar(25) NOT NULL,
    R_lname varchar(25) NOT NULL,
    R_phno varchar(25) NOT NULL,
    PRIMARY KEY (R_id),
	FOREIGN KEY (Username) REFERENCES tbl_Login(Username)
)



